<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>BuffImage</servlet-name>
    <servlet-class>com.test.hw.BufferImage</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CheckServlet</servlet-name>
    <servlet-class>com.test.hw.CheckServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>BuffImage</servlet-name>
    <url-pattern>/servlet/BuffImage</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>CheckServlet</servlet-name>
    <url-pattern>/servlet/CheckServlet</url-pattern>
  </servlet-mapping>
  
</web-app>
